<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.skgasutils.common.mapper.ComMapper">

    <!--EvuEmpId 중복 유무  -->
    <select id="countEvuEmpId" parameterType="hashmap" resultType="int" >
        select count(*)
        from EVU_EMP
        where EVU_EMP_ID = #{evuEmpId} and EVU_STD_ID = #{evuStdId}
    </select>



</mapper>